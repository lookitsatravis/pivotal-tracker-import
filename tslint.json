{
  "extends": [
    "tslint:latest",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": [
      true,
      {
        "singleQuote": true,
        "trailingComma": "es5",
        "printWidth": 130
      }
    ],
    "import-name": false,
    "no-implicit-dependencies": [
      true,
      "dev"
    ],
    "no-var-requires": false
  },
  "linterOptions": {
    "exclude": []
  }
}
